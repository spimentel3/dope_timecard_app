            locked = true;
        }
    });

    $("#btn_clear").click(function () {
        if (!locked) {
            $('#sat_start_hour').val("");
            $('#sat_start_minute').val("");
            $('#sat_end_hour').val("");
            $('#sat_end_minute').val("");
            $('#sat_break_hour').val("");
            $('#sat_break_minute').val("");

            $('#sun_start_hour').val("");
            $('#sun_start_minute').val("");
            $('#sun_end_hour').val("");
            $('#sun_end_minute').val("");
            $('#sun_break_hour').val("");
            $('#sun_break_minute').val("");

            $('#mon_start_hour').val("");
            $('#mon_start_minute').val("");
            $('#mon_end_hour').val("");
            $('#mon_end_minute').val("");
            $('#mon_break_hour').val("");
            $('#mon_break_minute').val("");

