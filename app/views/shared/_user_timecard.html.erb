                $("#wed_hours").html(json["wed_total"]);
                $("#thu_hours").html(json["thu_total"]);
                $("#fri_hours").html(json["fri_total"]);
                $("#sat_overtime").html(json["sat_overtime"]);
                $("#sun_overtime").html(json["sun_overtime"]);
                $("#mon_overtime").html(json["mon_overtime"]);
                $("#tue_overtime").html(json["tue_overtime"]);
                $("#wed_overtime").html(json["wed_overtime"]);
                $("#thu_overtime").html(json["thu_overtime"]);
                $("#fri_overtime").html(json["fri_overtime"]);
                $("input").removeClass("valid invalid");
                $("#sick_hours_input").val("0");
            }
            , error: function (XMLHttpRequest, textStatus, errorThrown) {
                console.log('Upload error: ' + XMLHttpRequest.responseText);
            }
        });
    }

