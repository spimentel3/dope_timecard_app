                thu_start: "<%= @timecard.thu_start.strftime("%Y-%m-%d") %> 00:00:00",
                thu_end: "<%= @timecard.thu_end.strftime("%Y-%m-%d") %> 00:00:00",
                thu_break_hours: "00",
